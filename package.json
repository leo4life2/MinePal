{
  "name": "minepal",
  "version": "0.4.2",
  "description": "AI minecraft friend",
  "type": "module",
  "main": "main.js",
  "author": "NemosAI",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.48.1",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "electron-updater": "^6.3.9",
    "express": "^4.19.2",
    "lowdb": "^7.0.1",
    "mic": "^2.1.2",
    "minecraft-data": "^3.67.0",
    "mineflayer-armor-manager": "^2.0.1",
    "minecraft-protocol": "file:libs/minecraft-protocol",
    "mineflayer": "file:libs/mineflayer",
    "mineflayer-auto-eat": "file:libs/mineflayer-auto-eat",
    "mineflayer-collectblock": "file:libs/mineflayer-collectblock",
    "mineflayer-pvp": "file:libs/mineflayer-pvp",
    "minecraft-protocol-forge": "file:libs/node-minecraft-protocol-forge",
    "prismarine-world": "file:libs/prismarine-world",
    "mineflayer-pathfinder": "file:mineflayer-pathfinder",
    "patch-package": "^8.0.0",
    "pidusage": "^3.0.2",
    "prismarine-item": "^1.14.0",
    "rotating-file-stream": "^3.2.3",
    "uiohook-napi": "^1.5.4",
    "vectra": "^0.9.0",
    "ws": "^8.18.0",
    "yargs": "^17.7.2"
  },
  "scripts": {
    "postinstall": "patch-package",
    "start": "electron .",
    "clean": "rm -rf dist",
    "buildLocal": "npm run clean && electron-builder",
    "build:mac": "electron-builder --mac",
    "build:linux": "electron-builder --linux",
    "build:win": "electron-builder --win",
    "build:all": "npm run build:mac && npm run build:linux && npm run build:win"
  },
  "devDependencies": {
    "@electron/notarize": "^2.3.2",
    "electron": "^31.7.7",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9"
  },
  "build": {
    "asar": true,
    "asarUnpack": [
      "node_modules/uiohook-napi/**"
    ],
    "appId": "com.nemosai.minepal",
    "productName": "Minepal",
    "afterSign": "notarize.js",
    "directories": {
      "buildResources": "build"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "main.js",
      "server.js",
      "ethan.json",
      "src/**/*",
      "frontend/dist/**/*",
      "bots/**/*",
      "mineflayer-pathfinder/**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/.bin",
      "!**/node_modules/*/package-lock.json"
    ],
    "mac": {
      "category": "public.app-category.games",
      "icon": "build/icon.icns",
      "entitlements": "build/entitlements.plist",
      "hardenedRuntime": true,
      "target": [
        "default"
      ]
    },
    "win": {
      "target": [
        "default"
      ],
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "build"
    }
  }
}
